<Window x:Class="LockScreenPictureGrabber.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Lock Screen Image Saver" Height="664" Width="796">
    <Grid>
        <ListBox Height="554" HorizontalAlignment="Left" Margin="0,32,0,0" Name="listBox1" Background="LightGray" VerticalAlignment="Top" Width="774" ScrollViewer.HorizontalScrollBarVisibility="Disabled">
            <ListBox.ItemsPanel>
                <ItemsPanelTemplate>
                    <WrapPanel IsItemsHost="True" />
                </ItemsPanelTemplate>
            </ListBox.ItemsPanel>
        </ListBox>
        <Button Content="Save" Height="33" HorizontalAlignment="Left" Margin="633,592,0,0" Name="button1" VerticalAlignment="Top" Width="141" Click="button1_Click" />
        <Label Height="28" HorizontalAlignment="Left" Name="label1" VerticalAlignment="Top" Width="762" />
        <TextBox Height="31" HorizontalAlignment="Left" Margin="0,594,0,0" Name="textBox1" VerticalAlignment="Top" Width="627" />
    </Grid>
</Window>
